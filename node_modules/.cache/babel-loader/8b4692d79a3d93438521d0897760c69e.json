MAAA,KAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,UAAA,CAAA,KADA,CACA;;AADA;AADA,KAAA,EAIA,CACA;AACA,IAAA,aAAA,CAAA,KAAA,UAAA,CAAA,IAAA,EAAA;AACA,MAAA,KAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,UAAA,CAAA,KADA,CACA;;AADA,OADA;AAIA,MAAA,EAAA,EAAA;AACA,QAAA,KAAA,EAAA,eAAA,IAAA,EAAA;AAAA;AACA,UAAA,KAAA,CAAA,UAAA,CAAA,KAAA,GAAA,IAAA;AACA;AAHA;AAJA,KAAA,EAUA,KAAA,UAAA,CAAA,UAAA,IAAA,KAAA,UAAA,CAAA,UAAA,CAAA,GAAA,CAAA,UAAA,MAAA,EAAA;AACA;AACA,aAAA,aAAA,CAAA,WAAA,EAAA;AAAA,QAAA,KAAA,EAAA;AAAA,UAAA,KAAA,EAAA,MAAA,CAAA,KAAA;AAAA,UAAA,KAAA,EAAA,MAAA,CAAA;AAAA;AAAA,OAAA,CAAA;AACA,KAHA,CAVA,CAFA,CAJA,CAAA;AAqBA;AA7CA,CAAA","sourcesContent":["\n<script>\nimport config from '../config'\nexport default {\n  name: 'FormItemDemo',\n  props: {\n    itemConfig: {\n      type: Object,\n      default: () => {}\n    }\n  },\n  computed: {\n    componentShow() {\n      const vIfArr = this.itemConfig?.options.vIf\n      if (!vIfArr) return true\n      const relationArr = config.filter(config => vIfArr.some(vIf => vIf.relationKey === config.formKey))\n      for (const relationItem of relationArr) {\n        const vIfItem = vIfArr.find(_ => _.relationKey === relationItem.formKey)\n        // 这里就是判断 联动的表单值 是否不满足 可以显示 的条件，不满足则不显示\n        if (relationItem.value !== vIfItem.value) return false\n      }\n      return true\n    }\n  },\n  render(createElement) {\n    if (!this.componentShow) return\n    console.log(this.itemConfig)\n    return createElement('el-form-item', {\n      props: {\n        label: this.itemConfig.label // 表单项的label\n      }\n    }, [\n      // 表单组件\n      createElement(this.itemConfig.type, {\n        props: {\n          value: this.itemConfig.value // 这里是自己实现一个 v-model\n        },\n        on: {\n          input: (nVal) => { // 这里是自己实现一个 v-model\n            this.itemConfig.value = nVal\n          }\n        }\n\n      }, this.itemConfig.optionData && this.itemConfig.optionData.map(option => {\n        // 这里只是 本demo 处理 el-select 的 option 数据，实际大家根据具体业务来实现即可\n        return createElement('el-option', { props: { label: option.label, value: option.value }})\n      }))\n    ])\n  }\n}\n</script>\n<style lang='scss' scoped>\n.app-container {}\n</style>\n"],"sourceRoot":"src/views/formConfig/FormItemDemo"}]}